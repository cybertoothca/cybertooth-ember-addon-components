<div class="container">
  <div class="navbar-header">
    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
            data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    {{#link-to "index" classNames="navbar-brand"}}
      <i class="fa fa-fw fa-desktop"></i>
      {{packageName}}
    {{/link-to}}
  </div>

  <!-- Collect the nav links, forms, and other content for toggling -->
  <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
    <ul class="nav navbar-nav navbar-right">
      <li>
        <a href="http://docs.{{packageName}}.cybertooth.io">
          Docs
        </a>
      </li>
      {{#if hasBlock}}
        {{yield}}
      {{else}}
        <li>
          {{#link-to "index" classNames="navbar-brand"}}
            Demo
          {{/link-to}}
        </li>
      {{/if}}
      <li>
        <a href="https://github.com/{{organizationName}}/{{packageName}}" target="_blank">
          GitHub
        </a>
      </li>
      <li>
        <a href="https://www.npmjs.com/package/{{packageName}}" target="_blank">
          npm
        </a>
      </li>
      <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown"
           aria-expanded="false">
          Other Projects <span class="caret"></span>
        </a>
        <ul class="dropdown-menu">
          <li class="dropdown-header">Bootstrap 3</li>
          <li>{{a-ember-observer packageName="ember-cli-bootstrap3-carousel"}}</li>
          <li>{{a-ember-observer packageName="ember-cli-bootstrap3-grid"}}</li>
          <li>{{a-ember-observer packageName="ember-cli-bootstrap3-popover"}}</li>
          <li>{{a-ember-observer packageName="ember-cli-bootstrap3-sass"}}</li>
          <li>{{a-ember-observer packageName="ember-cli-bootstrap3-tooltip"}}</li>
          <li role="separator" class="divider"></li>
          <li class="dropdown-header">Ember Data</li>
          <li>{{a-ember-observer packageName="ember-data-bootstrap3-forms"}}</li>
          <li role="separator" class="divider"></li>
          <li class="dropdown-header">Font Awesome</li>
          <li>{{a-ember-observer packageName="ember-cli-font-awesome-sass"}}</li>
          <li role="separator" class="divider"></li>
          <li class="dropdown-header">Input/Textarea</li>
          <li>{{a-ember-observer packageName="ember-cli-date-textbox"}}</li>
          <li>{{a-ember-observer packageName="ember-cli-text-support-mixins"}}</li>
          <li>{{a-ember-observer packageName="ember-cli-textarea-autosize"}}</li>
          <li role="separator" class="divider"></li>
          <li class="dropdown-header">Formatting</li>
          <li>{{a-ember-observer packageName="ember-helper-locale-date"}}</li>
          <li>{{a-ember-observer packageName="ember-helper-locale-number"}}</li>
          <li>{{a-ember-observer packageName="ember-cli-marked-down"}}</li>
        </ul>
      </li>
    </ul>
  </div>
</div>
